    <description>Profile Verify</description>
    <author>mooSocial - Tan.TT</author>
    <website>www.moosocial.com</website>
    <bootstrap>1</bootstrap>
    <routes>1</routes>
    <settings>
        <setting>
            <label>Enable Profile Verify</label>
            <name>verify_profile_enable</name>
            <description>Enable Profile Verify</description>
            <type>radio</type>
            <values>
                <value>
                    <name>Disable</name>
                    <value>0</value>
                    <select>0</select>
                </value>
                <value>
                    <name>Enable</name>
                    <value>1</value>
                    <select>1</select>
                </value>
            </values>
        </setting>
        <setting>
            <label>Enable auto verify based on user reviews</label>
            <name>verify_profile_auto_verify_after_review</name>
            <description>Enable auto verify based on user reviews</description>
            <type>text</type>
            <values>0</values>
        </setting>
        <setting>
            <label>Min review</label>
            <name>verify_profile_number_review</name>
            <description>Min review</description>
            <type>text</type>
            <values>5</values>
        </setting>
        <setting>
            <label>Min average score</label>
            <name>verify_profile_average_review</name>
            <description>Min average score</description>
            <type>text</type>
            <values>5</values>
        </setting>
        <setting>
            <label>Enable auto unverified if verified member does not match above settings</label>
